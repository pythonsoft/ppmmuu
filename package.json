{
  "name": "vue-starter-kit",
  "version": "1.0.0",
  "description": "vue starter kit",
  "main": "index.js",
  "scripts": {
    "build_deploy": "cross-env NODE_ENV=online node src/build.js",
    "build": "cross-env NODE_ENV=online node src/build.js",
    "buildP": "node src/build.js",
    "buildD": "node src/build.js && cp -rfv build/api/* ../UMP-FE/src/fe/api/",
    "dev": "cross-env NODE_ENV=development node src/build.js && cross-env NODE_ENV=development node src/server/reloadServer.js",
    "deploy": "pm2 startOrReload pm2.json --env production --update-env",
    "test": "cross-env NODE_ENV=test mocha \"src/server/api/*/test.js\"  \"src/server/*/test.js\" --delay --timeout 15000 --exit",
    "update": "npm install --registry=https://registry.npmmirror.com",
    "lint": "./node_modules/.bin/eslint --ignore-path .gitignore src/server",
    "lint:fix": "./node_modules/.bin/eslint --fix --ignore-path .gitignore src"
  },
  "author": "luoting",
